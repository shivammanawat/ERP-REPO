
<% include ../partials/header %>
<div class="container" style=" padding-top: 50px; ">
    <div class="row">
      
        <div style="width: 30%; margin: 25px auto;">
          <h1 style="text-align: center">Create a New Prospect</h1>
            <form action="/allprospectpage" method="POST">
            
                <div class="form-group">
                    <input class="form-control" type="text" name="id" placeholder="id" required>
                </div>
                
                <div class="form-group">
                    <input class="form-control" type="text" name="name" placeholder="name" required>
                </div>
                 <div class="form-group">
                    <input class="form-control" type="text" name="description" placeholder="description" required>
                </div>
                 <div class="form-group">
                    <input class="form-control" type="text" name="handler" placeholder="handle by" required>
                </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">ADD</button>
                </div>
            </form>
            <a href="/allprospectpage">Go Back</a>
        </div>
    </div>
</div>
<% include ../partials/footer %>

